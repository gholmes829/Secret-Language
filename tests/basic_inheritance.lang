# inheritance

class Dinosaur {
    void talk() {
        print('RROOOOAAAAARRRR!!!')
    }
    void feast() {
        print('MUUUNCHHHH')
    }
}

class Chicken(Dinosaur) {
    void feast() {
        print('nibble...')
    }
}

num main() {
    let dino = Dinosaur()
    print(dino)
    dino.talk()
    dino.feast()

    let chick = Chicken()
    print(chick)
    chick.talk()
    chick.feast()

    return 0
}

main()