# lets see if closures work

([] -> void) make_counter() {
    let i = 0
    void count() {
        i = i + 1
        print(i)
    }
    return count
}

num main() {
    let counter = make_counter()
    counter()  # prints 1
    counter()  # prints 2
    counter()  # prints 3
    counter()  # prints 4
    counter()  # prints 5
}

main()