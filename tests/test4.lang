// basic conditionals

num main() {
    if true {
        print(0)
    }

    if false {
        print(1)
    }
    else if false {
        print(2)
    }
    else if true {
        print(3)
    }
    else if false {
        print(4)
    }
    else {
        print(5)
    }

    if false {
        print(6)
    }
    else {
        print(7)
    }

    if false {
        print(8)
    }
    else if false {
        print(9)
    }
    else {
        print(10)
    }


}